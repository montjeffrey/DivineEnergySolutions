---
to: <%- route -%>.extension.ts
---
import { extensions } from '@wix/astro/builders';

export default extensions.dashboardMenuPlugin({
  id: '<%- config.id -%>',
  title: '<%- config.title -%>',
  extends: '<%- config.extends -%>',
  iconKey: '<%- config.iconKey -%>',
  action: <% if (type === 'OpenModal') { %>{
    openModal: {
      componentId: '<%- config.action.openModal.componentId -%>',
    }
  }<% } else { %>{
    navigateToPage: {
      pageId: '<%- config.action.navigateToPage.pageId -%>',
    }
  }<% } %>,
});