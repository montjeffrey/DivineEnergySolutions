---
to: <%- route -%>.extension.ts
---
import { extensions } from '@wix/astro/builders';

export default extensions.sitePlugin({
  id: '<%- config.id -%>',
  name: '<%- config.name -%>',
  marketData: {
    name: '<%- config.name -%>',
    description: 'Marketing Description',
    logoUrl: '{{BASE_URL}}/<%- route -%>-logo.svg',
  },
  placements: [{
    appDefinitionId: '<%- config.placements[0].appDefinitionId -%>',
    widgetId: '<%- config.placements[0].widgetId -%>',
    slotId: '<%- config.placements[0].slotId -%>',
  }],
  installation: { autoAdd: true },
  tagName: '<%- route -%>',
  element: '<%- config.element -%>',
  settings: '<%- config.settings -%>',
});
